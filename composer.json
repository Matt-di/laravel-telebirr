{
    "name": "matirezzo/laravel-telebirr",
    "description": "Telebirr payment gateway integration for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt-di",
            "email": "matirezzone6@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "phpseclib/phpseclib": "^3.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/events": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Telebirr\\LaravelTelebirr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Telebirr\\LaravelTelebirr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Telebirr\\LaravelTelebirr\\TelebirrServiceProvider"
            ],
            "aliases": {
                "Telebirr": "Telebirr\\LaravelTelebirr\\Telebirr"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html=coverage",
        "analyze": "phpstan analyse"
    }
}
